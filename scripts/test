    }
    spinnerGradient.Parent = spinner
    
    local buttonsContainer = Instance.new("Frame")
    buttonsContainer.Size = UDim2.new(1, 0, 0, Config.ButtonHeight)
    buttonsContainer.Position = UDim2.new(0, 0, 0, isMobileDevice and 230 or 260)
    buttonsContainer.BackgroundTransparency = 1
