    savedKeyIndicator.TextColor3 = Colors.Success
    savedKeyIndicator.TextSize = Config.StatusTextSize
    savedKeyIndicator.Font = Enum.Font.Gotham
    savedKeyIndicator.TextXAlignment = Enum.TextXAlignment.Center
    savedKeyIndicator.ZIndex = 12
    savedKeyIndicator.Parent = content
    
    if getSavedKey() then
        savedKeyIndicator.Text = "âœ“ Saved key found"
    end

    UI.Content = content
    UI.SavedKeyIndicator = savedKeyIndicator
    return content
