    buttonsContainer.Parent = parent
    
    local getKeyButton = Instance.new("TextButton")
    getKeyButton.Size = UDim2.new(0.48, 0, 1, 0)
    getKeyButton.BackgroundColor3 = Colors.GetKey
    getKeyButton.BorderSizePixel = 0
    getKeyButton.Text = "Get Key"
    getKeyButton.TextColor3 = Colors.TextPrimary
    getKeyButton.TextSize = Config.ButtonTextSize
    getKeyButton.Font = Enum.Font.GothamMedium
    getKeyButton.AutoButtonColor = false
    getKeyButton.ZIndex = 13
    getKeyButton.Selectable = true
    getKeyButton.Parent = buttonsContainer
    
    local getKeyCorner = Instance.new("UICorner")
    getKeyCorner.CornerRadius = UDim.new(0, isMobileDevice and 8 or 10)
    getKeyCorner.Parent = getKeyButton
    
    local discordButton = Instance.new("TextButton")
    discordButton.Size = UDim2.new(0.48, 0, 1, 0)
    discordButton.Position = UDim2.new(0.52, 0, 0, 0)
    discordButton.BackgroundColor3 = Colors.Discord
    discordButton.BorderSizePixel = 0
    discordButton.Text = "Discord"
    discordButton.TextColor3 = Colors.TextPrimary
    discordButton.TextSize = Config.ButtonTextSize
    discordButton.Font = Enum.Font.GothamMedium
    discordButton.AutoButtonColor = false
    discordButton.ZIndex = 13
    discordButton.Selectable = true
    discordButton.Parent = buttonsContainer
    
    local discordCorner = Instance.new("UICorner")
    discordCorner.CornerRadius = UDim.new(0, isMobileDevice and 8 or 10)
    discordCorner.Parent = discordButton

    local forgetButton = Instance.new("TextButton")
    forgetButton.Size = UDim2.new(0, 120, 0, 30)
    forgetButton.Position = UDim2.new(0.5, -60, 0, Config.ButtonHeight + 10)
    forgetButton.BackgroundColor3 = Colors.Warning
    forgetButton.BorderSizePixel = 0
    forgetButton.Text = "Forget Key"
    forgetButton.TextColor3 = Colors.TextPrimary
    forgetButton.TextSize = isMobileDevice and 12 or 14
    forgetButton.Font = Enum.Font.GothamMedium
    forgetButton.AutoButtonColor = false
    forgetButton.ZIndex = 13
    forgetButton.Selectable = true
    forgetButton.Parent = buttonsContainer
    
    local forgetCorner = Instance.new("UICorner")
    forgetCorner.CornerRadius = UDim.new(0, 8)
    forgetCorner.Parent = forgetButton

    UI.Buttons = {
        Submit = submitButton,
        GetKey = getKeyButton,
        Discord = discordButton,
        Forget = forgetButton,
        Loading = {Container = loadingContainer, Spinner = spinner}
    }
    
    return {submitButton, getKeyButton, discordButton, forgetButton}
end

