--[[
    LS Hub Key System dengan Fitur Pengingat Key (Versi Diperbaiki)
    
    PERUBAHAN:
    - DataStoreService diganti dengan writefile/readfile karena DataStore tidak bisa diakses dari LocalScript.
    - Key sekarang disimpan di file "LSHub_SavedKey.txt" di folder executor Anda.
    - Script akan otomatis mencoba menggunakan key yang tersimpan saat dijalankan ulang.
--]]

local Services = {
    Players = game:GetService("Players"),
    TweenService = game:GetService("TweenService"),
    UserInputService = game:GetService("UserInputService"),
    RunService = game:GetService("RunService"),
    HttpService = game:GetService("HttpService"),
    RbxAnalyticsService = game:GetService("RbxAnalyticsService"),
    GuiService = game:GetService("GuiService")
    -- DataStoreService DIHAPUS karena tidak bisa diakses dari client
}

local Player = Services.Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")

-- ========================================
-- DETECT PLATFORM
-- ========================================

local function isMobile()
    return Services.UserInputService.TouchEnabled and not Services.UserInputService.KeyboardEnabled
end

local isMobileDevice = isMobile()

-- ========================================
-- KEY SYSTEM CONFIGURATION
-- ========================================

local urlBase = "https://louissxe-lshub.up.railway.app"

-- ðŸ§  Function safe HTTP
local function httpGetSafe(url)
    local ok,res = pcall(function() return game:HttpGet(url) end)
    if ok then return true,res end
    ok,res = pcall(function() return Services.HttpService:GetAsync(url,true) end)
    if ok then return true,res end
    return false,res
end

-- ðŸ–¥ HWID simple (userId fallback)
local function getHWID()
    local success, clientId = pcall(function()
        return Services.RbxAnalyticsService:GetClientId()
    end)
    
    if success and clientId then
        return clientId
    else
        return tostring(Player.UserId)
    end
end

local hwid = getHWID()

-- ========================================
-- MODIFIKASI: FILE I/O FUNCTIONS (PENGGANTI DATASTORE)
-- ========================================

local keyFileName = "LSHub_SavedKey.txt"

-- Fungsi untuk menyimpan key yang valid ke file lokal
local function saveKey(key)
    local success, err = pcall(function()
        if syn then
            syn.writefile(keyFileName, key)
        else
            writefile(keyFileName, key)
        end
    end)
    if not success then
        warn("Gagal menyimpan key: " .. tostring(err))
    end
    return success
end

-- Fungsi untuk mengambil key yang tersimpan dari file lokal
local function getSavedKey()
    local success, key = pcall(function()
        -- Cek apakah file ada sebelum mencoba membaca
        if (syn and isfile(keyFileName)) or (not syn and isfile and isfile(keyFileName)) then
            if syn then
                return syn.readfile(keyFileName)
            else
                return readfile(keyFileName)
            end
        end
        return nil -- Kembalikan nil jika file tidak ada
    end)
    if success then
        return key
    end
    return nil
end

-- Fungsi untuk menghapus key yang tersimpan
local function removeSavedKey()
    local success, err = pcall(function()
        if (syn and isfile(keyFileName)) or (not syn and isfile and isfile(keyFileName)) then
            if syn then
                delfile(keyFileName)
            else
                delfile(keyFileName)
            end
        end
    end)
    return success
end

-- ========================================
-- MODIFIKASI: SAVED KEY CHECK (DIJALANKAN SEBELUM UI MUNCUL)
-- ========================================

local function trySavedKey()
    local savedKey = getSavedKey()
    if not savedKey then
        return false -- Tidak ada key tersimpan
    end

    -- Jika ada key tersimpan, coba verifikasi di background
    local username = Player.Name
    local verifyUrl = string.format(
        "%s/verify?key=%s&hwid=%s&user=%s",
        urlBase,
        Services.HttpService:UrlEncode(savedKey),
        Services.HttpService:UrlEncode(hwid),
        Services.HttpService:UrlEncode(username)
    )
    
    local ok, res = httpGetSafe(verifyUrl)
    if ok then
        local success, data = pcall(function() return Services.HttpService:JSONDecode(res) end)
        if success and data and data.ok and data.token then
            -- Key tersimpan masih valid! Jalankan script.
            local loadUrl = string.format("%s/load?token=%s", urlBase, Services.HttpService:UrlEncode(data.token))
            local ok2, scriptText = httpGetSafe(loadUrl)
            if ok2 and #scriptText > 10 then
                local fn = loadstring(scriptText)
                if fn then
                    pcall(fn)
                    return true -- Berhasil menjalankan script dengan key tersimpan
                end
            end
        end
    end

    -- Jika sampai di sini, berarti key tersimpan tidak valid lagi. Hapus.
    warn("Key tersimpan tidak valid, menghapus...")
    removeSavedKey()
    return false
end

-- Coba jalankan fungsi di atas. Jika berhasil, script akan berhenti di sini.
if trySavedKey() then
    return -- Keluar dari script karena sudah dijalankan
end

-- ================== ORIGINAL HWID AUTO LOGIN ==================
-- (Ini akan dijalankan jika tidak ada key tersimpan atau key tersimpan tidak valid)
local autoUrl = string.format("%s/autologin?hwid=%s", urlBase, Services.HttpService:UrlEncode(hwid))
local ok,res = httpGetSafe(autoUrl)

if ok then
    local success,data = pcall(function() return Services.HttpService:JSONDecode(res) end)
    if success and data and data.ok and data.token then
        local loadUrl = string.format("%s/load?token=%s", urlBase, Services.HttpService:UrlEncode(data.token))
        local ok2, scriptText = httpGetSafe(loadUrl)
        if ok2 and #scriptText > 10 then
            local fn = loadstring(scriptText)
            if fn then
                pcall(fn)
                return -- âœ… langsung jalanin script
            end
        end
    end
end

